<!DOCTYPE html>
<html>
<head>
	<title>Edit Employer</title>
</head>
<body>
	<form method="post">
		<fieldset>
			<legend>Edit Employer</legend>
			<table>
				<tr>
					<td>Employer ID:</td>
					<td><input type="number" name="empId" value="<%= employer.id %>" readonly></td>
				</tr>
				<tr>
					<td>Username:</td>
					<td><input type="text" name="username" value="<%= employer.username %>"></td>
				</tr>
				<tr>
					<td>Password:</td>
					<td><input type="text" name="password" value="<%= employer.password %>"></td>
				</tr>
				<tr>
					<td>Employer Name:</td>
					<td><input type="text" name="empName" value="<%= employer.emp_name %>"></td>
				</tr>
				<tr>
					<td>Company Name:</td>
					<td><input type="text" name="compName" value="<%= employer.comp_name %>"></td>
                </tr>
                <tr>
					<td>Contact No:</td>
					<td><input type="number" name="contactNo" value="<%= employer.contact_no %>"></td>
				</tr>
				<tr>
					<td></td>
					<td><input type="submit" name="submit" value="Update"></td>
				</tr>
			</table>
		</fieldset>
    </form>
    

    <% alert %> 
</body>
</html>